CLANG_ENABLE_MODULES = YES
CLANG_ENABLE_OBJC_ARC = YES
FRAMEWORK_SEARCH_PATHS = $(inherited) "${PODS_ROOT}/ArcGIS-Runtime-SDK-iOS"
GCC_PREPROCESSOR_DEFINITIONS = $(inherited) COCOAPODS=1
HEADER_SEARCH_PATHS = $(inherited) "${PODS_ROOT}/Headers/Public" "${PODS_ROOT}/Headers/Public/ArcGIS-Runtime-SDK-iOS"
LD_RUNPATH_SEARCH_PATHS = $(inherited) '@executable_path/Frameworks' '@loader_path/Frameworks'
OTHER_CFLAGS = $(inherited) -iquote "$CONFIGURATION_BUILD_DIR/Firebase.framework/Headers" -isystem "${PODS_ROOT}/Headers/Public" -isystem "${PODS_ROOT}/Headers/Public/ArcGIS-Runtime-SDK-iOS"
OTHER_LDFLAGS = $(inherited) -ObjC -l"c++" -framework "ArcGIS" -framework "Firebase"
PODS_FRAMEWORK_BUILD_PATH = "$(BUILD_DIR)/$(CONFIGURATION)$(EFFECTIVE_PLATFORM_NAME)/Pods-Shuttle"
PODS_ROOT = ${SRCROOT}/Pods
